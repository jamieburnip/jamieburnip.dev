    "gatsby-plugin-google-analytics": "^2.1.28",
    "gatsby-plugin-lodash": "^3.1.15",
    "gatsby-plugin-manifest": "^2.2.29",
    "gatsby-plugin-nprogress": "^2.1.14",
    "gatsby-plugin-offline": "^3.0.23",
