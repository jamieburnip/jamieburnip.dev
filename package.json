  "author": "Jamie Burnip <jamieburnip@gmail.com>",
  "repository": "git@github.com:jamieburnip/madebyjamie.dev",
  "dependencies": {
    "gatsby": "^2.18.3",
    "gatsby-image": "^2.2.33",
    "gatsby-plugin-catch-links": "^2.1.17",
