    "gatsby-remark-responsive-iframe": "^2.4.3",
    "gatsby-source-filesystem": "^2.3.8",
    "gatsby-transformer-remark": "^2.8.13",
    "gatsby-transformer-sharp": "^2.5.3",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-share": "^4.1.0",
    "react-twitter-widgets": "^1.7.1",
    "url-join": "^4.0.0",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.20.0",
    "gh-pages": "^3.0.0",
    "prettier": "^2.0.5",
    "remark-cli": "^8.0.0",
    "remark-preset-lint-recommended": "^4.0.0",
    "stylelint": "^13.5.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "write-good": "^1.0.1"
  },
  "keywords": [
    "jamie burnip",
    "development",
    "dev",
    "gatsby",
    "tutorials",
    "tech",
    "articles",
    "blog"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby clean && gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby clean && gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "gatsby clean",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "format:styles": "prettier '**/*.{css,scss}' --write"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
